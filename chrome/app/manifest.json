{
  "name": "Déjà Vu",
  "short_name": "Déjà Vu",
  "description": "Find anything you have ever browsed",
  "version": "0.2.1",
  "manifest_version": 2,
  "content_scripts": [{
      "js": [
        "content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "20": "icons/icon20.png",
      "24": "icons/icon24.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "64": "icons/icon64.png",
      "128": "icons/icon128.png",
      "256": "icons/icon256.png",
      "512": "icons/icon512.png"
    },
    "default_title": "Déjà Vu"
  },
  "permissions": [
    "tabs",
    "idle",
    "identity",
    "identity.email",
    "https://dejavu.ninja/"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "20": "icons/icon20.png",
    "24": "icons/icon24.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png",
    "256": "icons/icon256.png",
    "512": "icons/icon512.png"
  }
}