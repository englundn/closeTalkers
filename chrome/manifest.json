{
  "name": "Déjà vu",
  "description": "Search through all websites you've ever visited",
  "version": "0.1",
  "manifest_version": 2,
  "content_scripts": [{
      "js": [
        "content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }],
  "permissions": [
    "identity",
    "identity.email",
    "https://deja-vu.herokuapp.com/"
  ]
}